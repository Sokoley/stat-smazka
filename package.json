{
  "name": "stat-dashboard",
  "version": "1.0.0",
  "description": "Marketplace metrics dashboard",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "build:css": "tailwindcss -i ./src/public/css/input.css -o ./src/public/css/styles.css --minify",
    "build:tsx": "esbuild src/public/pricecheck/index.tsx --bundle --minify --outfile=src/public/pricecheck/index.js --external:react --external:react-dom --define:process.env.NODE_ENV=\\\"production\\\"",
    "build": "npm run build:css && npm run build:tsx",
    "watch:css": "tailwindcss -i ./src/public/css/input.css -o ./src/public/css/styles.css --watch"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "better-sqlite3": "^9.6.0",
    "chromedriver": "^145.0.4",
    "ejs": "^3.1.10",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "multer": "^2.0.2",
    "proxy-chain": "^2.7.1",
    "puppeteer": "^22.15.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "selenium-webdriver": "^4.40.0"
  },
  "devDependencies": {
    "esbuild": "^0.20.2",
    "nodemon": "^3.1.0",
    "tailwindcss": "^3.4.1"
  }
}
